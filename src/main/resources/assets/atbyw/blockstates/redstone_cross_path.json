{
  "multipart": [
    {
      "when": {
        "x": false,
        "z": false
      },
      "apply": {
        "model": "atbyw:block/redstone_cross_path_dot"
      }
    },
    {
      "when": {
        "powered_x": false,
        "x": true
      },
      "apply": {
        "model": "atbyw:block/redstone_cross_path_x"
      }
    },
    {
      "when": {
        "powered_x": true,
        "x": true
      },
      "apply": {
        "model": "atbyw:block/redstone_cross_path_on_x"
      }
    },

    {
      "when": {
        "powered_z": false,
        "z": true
      },
      "apply": {
        "model": "atbyw:block/redstone_cross_path_z"
      }
    },
    {
      "when": {
        "powered_z": true,
        "z": true
      },
      "apply": {
        "model": "atbyw:block/redstone_cross_path_on_z"
      }
    }
  ]
}