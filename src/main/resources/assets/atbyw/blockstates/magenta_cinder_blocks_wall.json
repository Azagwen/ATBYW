{
    "multipart": [
        {
            "when": {
                "up": "true"
            },
            "apply": {
                "model": "atbyw:block/concrete/cinder_blocks/wall/magenta_cinder_blocks_wall_post"
            }
        },
        {
            "when": {
                "up": "true",
                "post_slab": "true"
            },
            "apply": {
                "model": "atbyw:block/concrete/cinder_blocks/wall/magenta_cinder_blocks_wall_slab"
            }
        },
        {
            "when": {
                "north": "low"
            },
            "apply": {
                "model": "atbyw:block/concrete/cinder_blocks/wall/magenta_cinder_blocks_wall_side"
            }
        },
        {
            "when": {
                "east": "low"
            },
            "apply": {
                "model": "atbyw:block/concrete/cinder_blocks/wall/magenta_cinder_blocks_wall_side",
                "y": 90
            }
        },
        {
            "when": {
                "south": "low"
            },
            "apply": {
                "model": "atbyw:block/concrete/cinder_blocks/wall/magenta_cinder_blocks_wall_side",
                "y": 180
            }
        },
        {
            "when": {
                "west": "low"
            },
            "apply": {
                "model": "atbyw:block/concrete/cinder_blocks/wall/magenta_cinder_blocks_wall_side",
                "y": 270
            }
        },
        {
            "when": {
                "north": "tall"
            },
            "apply": {
                "model": "atbyw:block/concrete/cinder_blocks/wall/magenta_cinder_blocks_wall_side_tall",
                "uvlock": true
            }
        },
        {
            "when": {
                "east": "tall"
            },
            "apply": {
                "model": "atbyw:block/concrete/cinder_blocks/wall/magenta_cinder_blocks_wall_side_tall",
                "y": 90,
                "uvlock": true
            }
        },
        {
            "when": {
                "south": "tall"
            },
            "apply": {
                "model": "atbyw:block/concrete/cinder_blocks/wall/magenta_cinder_blocks_wall_side_tall",
                "y": 180,
                "uvlock": true
            }
        },
        {
            "when": {
                "west": "tall"
            },
            "apply": {
                "model": "atbyw:block/concrete/cinder_blocks/wall/magenta_cinder_blocks_wall_side_tall",
                "y": 270,
                "uvlock": true
            }
        }
    ]
}